{
  "name": "element-qsa-scope",
  "version": "1.0.0",
  "description": "Return elements matching a selector relative to the current node",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "jonathantneal/element-qsa-scope",
  "homepage": "https://github.com/jonathantneal/element-qsa-scope#readme",
  "bugs": "https://github.com/jonathantneal/element-qsa-scope/issues",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "npm run lint:ec && npm run lint:js",
    "lint:ec": "echint",
    "lint:js": "eslint *.js --cache --ignore-pattern .gitignore",
    "prepublish": "npm test",
    "test": "npm run lint"
  },
  "engines": {
    "node": ">=0.12"
  },
  "devDependencies": {
    "echint": "^2.1.1",
    "echint-config-dev": "^1.0.0",
    "eslint": "^3.14.0",
    "eslint-config-dev": "^1.0.0"
  },
  "echint": {
    "extends": "dev"
  },
  "eslintConfig": {
    "extends": "dev",
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^selectors$"
        }
      ]
    }
  },
  "keywords": [
    "scope",
    "dom",
    "element",
    "matches",
    "polyfill",
    "standard",
    "relative",
    "context",
    "queries",
    "query",
    "selectors"
  ]
}
